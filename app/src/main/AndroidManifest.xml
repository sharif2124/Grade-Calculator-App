<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sharif.cgpa">

    <application
        android:allowBackup="true"
        android:icon="@drawable/cgpa"
        android:label="@string/app_name"
        android:roundIcon="@drawable/cgpa"
        android:supportsRtl="true"
        android:theme="@style/Theme.GradeCalculationWithRoomDtabase">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.GradeCalculationWithRoomDtabase.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>